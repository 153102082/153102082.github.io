*{
    overflow: hidden;
}
body ,html{padding: 0;
margin:0;

}
body .wrap{
    overflow: hidden;

}

.wrap {
    color:#ccc;
    width: 100vw;
    height: auto;
    position: absolute;
    overflow: auto;
    left: 50%;
    transform: translate(-50%,0);
    bottom: 0;
    background: url("../img/bg.jpg") center no-repeat;
    background-size: 100% auto;

}

.last a{
    text-decoration: none;
    position: absolute;
    display: block;
    animation: cfont 1.5s linear 0s infinite;

    color:#fff;
    font-size:5vw;
    left:24vw;
    bottom:10vh;

}

.last-bg{
    text-align: center;
    position: relative;
}

.loading ,.last{

    width:100vw;
    height:100vh;

    margin:0 auto;
}
