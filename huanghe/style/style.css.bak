.wrap-ship{
    position:fixed;
    bottom:-1.5vh;
    left:30px;
    width:130vw;
    height:auto;
    z-index: 5;
}
.ship{

    transform: scale(0.5,0.5);
    width:100vw;
    height:auto;
    left:-4vw;



}
.content{
    width: 100%;
    height: 100%;
