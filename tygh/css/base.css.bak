select,input{border:none;outline:none;}
p{display: inline-block;}
a{text-decoration: none;color: #2da5e1;}
.top{line-height:30px;color:#868686;font-size:13px; }
.top .call{padding-left:20px;background:url(../img/icon-call.png) no-repeat left center}
.top .welcome{float:right;}

.top a{
    text-decoration:none;
    color:#2da5e1;
    padding-left:10px;
}
.header .logo{
    display:inline-block;
    width:402px;
    height:74px;
    padding:9px 0;

}
.header .logo img{width:100%;height:100%;}
.header .search{
    width:326px;
    height:38px;
    position:absolute;
    background-color:orange;
    right:0;
    top:29px;
}
.nav .link{
    color:#fff;
    display: inline-block;
    padding-left:30px;
    font-size:16px;
    line-height: 36px;
    min-width:80px;
    text-align:center;
    float:left;
}
.nav .right{float: right;}
.nav .menu{width:130px;background-color:#1fa4f1;padding-right:30px;position:relative;}
.menu-list{position:absolute;background:#1fa4f1;width:100%;left:0;height:423px;top:36px;z-index:2;}
.banner-search{
    background:#fafafa;
    width:250px;
    height:255px;
    float:right;
}
.banner-help{
    background:#fafafa;
    width:250px;
    height:146px;
    float:right;
    margin-top:12px;

}
.banner-search .capiton,.banner-help .capiton{
    text-align: center;
    padding:15px 0;
    height:22px;

}
.banner-search .capiton .text,.banner-help .capiton .text{
    font-size:16px;
    display:inline-block;
    height:22px;
    line-height:22px;
    padding-left:28px;
    color:#fec009;
    background:url(../img/icon-help.png) 0 0 no-repeat;
}
.banner-help .capiton .text{
    background-position: 0 -23px;
    color:#00b3ea;

}
.banner-search .form{
    height:150px;
}
.banner-search .form .line{
    text-align: center;
    padding-bottom:9px;

}
.banner-search .form .line select{
    width: 170px;
    font-size:12px;
    border:1px solid #dcdddd;
    height:26px;
    padding:2px 0;
    color:#666;
}
.banner-search .submit{
    height:32px;
    text-align: center;
}
.banner-search .submit .button{
    width:108px;
    height:33px;
    background-color:#fecd09;
    font-size:14px;
    color:#fff;
    border-radius: 3px;

}
.banner-help .list .link{
    display:inline-block;
    color:#00b3ea;
    font-size:14px;
    height:26px;
    width:86px;
    text-align: center;
    padding:0 0 8px 26px;
}
.content-tab .capiton{
    height:34px;
    line-height:34px;
    background: #f4f6fa;
}

.content-tab .capiton .item{
    display: inline-block;
    width:112px;
    height:34px;
    text-align: center;
    line-height: 34px;

}
.content-tab .capiton .item_focus{
    background: #60bff2;
    color:#fff;
}
.content-tab .block{
    border:1px solid #f4f6fa;
    height:452px;
}
.content-tab .block-capiton{
    padding:8px;
    height: 26px;

    border-bottom:1px solid #f4f6fa;
}
.content-tab .block-capiton .block-capiton-item{
    display: inline-block;
    line-height: 26px;
    padding:0 10px ;
    font-size: 12px;
    color:#4c4948;
}
.block-capiton-item_focus{
    background: #60bff2;
    color:#fff;
}
.content-tab .block-content{
    padding:16px 12px;
}
.content-tab .block-content .block-list .item{
    width:216px;
    height:102px;
    font-size:12px;
    float:left;
    padding:0 20px 10px 120px;
    position:relative;

}
.content-tab .block-content .block-list .item img{
    width:110px;
    height:98px;
    position: absolute;
    top:0;
    left:0;
}
.content-tab .block-content .block-list .item .item-name{
    font-size:14px;
    padding-top:13px;
    color:#036eb7;
    line-height:21px;



}
.content-tab .block-content .block-list .item .item-name .item-level{
    font-size:12px;
    color:#666;
    float:right;


}
.content-tab .block-content .block-list .item .item-phone,.content-tab .block-content .block-list .item .item-address{

    padding-bottom:4px;
    color:#666;
    line-height:18px;

}
.content-tab .block-text-list .item{
    display: inline-block;
    float:left;
    width:351px;
    height:27px;
    line-height:27px;
    padding-top:8px;
    font-size:12px;
    color:#666;
    border-bottom:1px dashed #dcdddd;

}
.content-tab .block-text-list .item:nth-child(2n){
    margin-left:13px;
}
.content-tab .block-text-list .item .level{
    float:right;

}
.content-tab .block-content .block-more{
    line-height:55px;
    display:block;
    text-align: center;
    color:#5084c6;
    font-size:14px;

}
.content-news .capiton,.content-close .capiton{
    font-size:15px;
    text-indent:20px;
    height:38px;
    line-height: 38px;
    background: #f4f6fa;



}
.content-news .capiton .more,.content-close .capiton .more{
    font-size:13px;
    color:#333;
    float:right;
    padding-right:25px;

}
.content-news .capiton .more:before,.content-close .capiton .more:before{
    content:'|';
}
.content-news .capiton {
    color:#fec009;
}
.content-close .capiton{
    color:#00b3ea;
}
.content-news .list,.content-close .list{
    padding:15px 20px 13px 35px;
    line-height:29px;
    background:url(../img/list-yellow.jpg) 17px 20px no-repeat;
}
.content-news .list .link,.content-close .list .link{
    display:block;
    height:29px;
    font-size:12px;
    color:#969696;
    overflow: hidden;



}
.block-wrap:nth-child(n+3){
    display:none;


}
